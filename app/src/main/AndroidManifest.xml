<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.demoappbasic">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppNoActionBar">
        <activity android:name=".activity.Service_Activity"></activity>
        <activity android:name=".menu.Popup_menuActivity" />
        <activity android:name=".menu.Context_menuActivity" />
        <activity android:name=".menu.Option_menuActivity" />
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.Registration" />
        <activity android:name=".activity.NavigationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>